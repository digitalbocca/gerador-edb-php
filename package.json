{
  "name": "gerador-edb",
  "version": "0.10.0",
  "description": "Gerador de Páginas HTML Estáticas em PHP",
  "main": "gerador.php",
  "config": {
    "commitMessage": "Adicionado Gerador de README"
  },
  "scripts": {
    "gerar": "php gerador.php",
    "git": "npm run gitstatus && npm run gitadd && npm run gitcommit && npm run gittag && npm run gitpushorigin && npm run gitpushbitbucket",
    "gitstatus": "git status",
    "gitadd": "git add .",
    "gitcommit": "git commit -m \"%npm_package_config_commitMessage%\"",
    "gittag": "git tag v%npm_package_version%",
    "gitpushorigin" : "git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://digitalbocca@bitbucket.org/digitalbocca/gerador-edb.git"
  },
  "keywords": [
    "gerador"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "ISC",
  "homepage": "https://bitbucket.org/digitalbocca/gerador-edb#readme"
}
