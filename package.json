{
  "name": "gerador-edb",
  "version": "1.3.0",
  "description": "Gerador de Páginas HTML Estáticas em PHP",
  "main": "gerador.php",
  "config": {
    "commitMessage": "Adicionado cross-var-no-babel@1.2.0"
  },
  "scripts": {
    "gerar": "php gerador.php",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo && npm run gpb",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpo": "git push origin master --tags",
    "gpb": "git push bitbucket master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/gerador-edb-php.git"
  },
  "keywords": [
    "gerador"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "MIT",
  "homepage": "https://github.com/digitalbocca/gerador-edb-php.git",
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0"
  }
}
